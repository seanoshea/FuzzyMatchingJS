{
  "name": "fuzzymatchingjs",
  "version": "0.1.0",
  "description": "String Fuzzy Matching",
  "main": "index.js",
  "files": [
    "index.js",
    "lib",
    "README.md",
    "LICENCE",
    "dist/fuzzymatching.js",
    "dist/fuzzymatching.min.js"
  ],
  "scripts": {
    "lint": "eslint src/lib/*.js test",
    "clean:node": "rm -rf index.js lib",
    "clean:browser": "rm -rf dist",
    "clean": "npm run clean:node && npm run clean:browser",
    "minify": "uglifyjs dist/fuzzymatching.js -o dist/fuzzymatching.min.js  --compress --mangle --comments '/Copyright/'",
    "build:browser": "babel-node build-browser && npm run minify",
    "build:node": "babel src -d . --presets es2015 --plugins add-module-exports",
    "build": "npm run build:browser && npm run build:node",
    "compressed-size": "echo 'dist/fuzzymatching.min.js gzipped will weigh' $(cat dist/fuzzymatching.min.js | gzip -9 | wc -c) bytes",
    "generate-docs": "jsdoc -c ./docs/conf.json -d generated-docs -t ./node_modules/ink-docstrap/template -R docs/README.md -r ./src",
    "pretest": "npm run lint && npm run build",
    "test": "npm run jest --coverage --coveragePathIgnorePatterns dist/*.min.js",
    "coveralls": "npm run jest --config test/jest_coveralls.json --coverage",
    "start": "webpack-dev-server --progress --colors"
  },
  "bugs": {
    "url": "http://github.com/seanoshea/fuzzymatchingjs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seanoshea/fuzzymatchingjs.git"
  },
  "keywords": [
    "fuzzy",
    "match",
    "string",
    "matching"
  ],
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-runtime": "^6.23.0",
    "coveralls": "^2.11.9",
    "eslint": "^3.8.1",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.0.1",
    "ink-docstrap": "^1.3.0",
    "jasmine-core": "^2.6.2",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "jsdoc": "^3.4.3",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-jasmine": "^1.1.0",
    "prettier": "^1.3.1",
    "pretty-bytes": "^4.0.2",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.5.1",
    "uglify-js": "^3.0.11",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "author": "Sean O'Shea <oshea.ie@gmail.com>",
  "license": "Apache-2.0",
  "homepage": "https://github.com/seanoshea/fuzzymatchingjs#readme"
}
