{
  "name": "fuzzymatchingjs",
  "version": "0.3.0",
  "description": "String Fuzzy Matching",
  "main": "dist/fuzzymatchingjs.cjs.js",
  "module": "dist/fuzzymatchingjs.esm.js",
  "browser": "dist/fuzzymatchingjs.umd.js",
  "scripts": {
    "lint": "eslint src/lib/*.js",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "prebuild": "npm run lint",
    "pretest": "npm run build",
    "compressed-size": "echo 'dist/fuzzymatching.min.js gzipped will weigh' $(cat dist/fuzzymatching.min.js | gzip -9 | wc -c) bytes",
    "generate-docs": "jsdoc -c ./docs/conf.json -d generated-docs -t ./node_modules/ink-docstrap/template -R docs/README.md -r ./src"
  },
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "http://github.com/seanoshea/fuzzymatchingjs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seanoshea/fuzzymatchingjs.git"
  },
  "keywords": [
    "fuzzy",
    "match",
    "string",
    "matching"
  ],
  "jest": {
    "verbose": true,
    "coverageDirectory": "coverage",
    "collectCoverage": true,
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/dist/fuzzymatchingjs.umd.js",
      "<rootDir>/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 90,
        "lines": 90,
        "statements": -10
      }
    }
  },
  "devDependencies": {
    "@babel/preset-env": "^7.9.6",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^26.0.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-runtime": "^6.26.0",
    "codecov": "^3.6.5",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "ink-docstrap": "^1.3.2",
    "jasmine-core": "^3.5.0",
    "jest": "^26.0.0",
    "jest-cli": "^26.0.0",
    "jsdoc": "^3.6.4",
    "pretty-bytes": "^5.3.0",
    "rollup": "^2.7.6",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0"
  },
  "author": "Sean O'Shea <oshea.ie@gmail.com>",
  "license": "Apache-2.0",
  "homepage": "https://github.com/seanoshea/fuzzymatchingjs#readme"
}
