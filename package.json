{
  "name": "fuzzymatchingjs",
  "version": "0.1.0",
  "description": "String Fuzzy Matching",
  "main": "index.js",
  "files": [
    "index.js",
    "lib",
    "README.md",
    "LICENCE",
    "fuzzymatching.js",
    "fuzzymatching.min.js"
  ],
  "scripts": {
    "lint": "eslint src test",
    "clean:node": "rm -rf index.js lib",
    "clean:browser": "rm -rf fuzzymatching*.js",
    "clean": "yarn clean:node && yarn clean:browser",
    "minify": "uglifyjs fuzzymatching.js -o fuzzymatching.min.js  --compress --mangle --comments '/Copyright/'",
    "build:browser": "babel-node build-browser && yarn minify",
    "build:node": "babel src -d . --presets es2015 --plugins add-module-exports",
    "build": "yarn build:browser && yarn build:node",
    "compressed-size": "echo 'fuzzymatching.min.js gzipped will weigh' $(cat fuzzymatching.min.js | gzip -9 | wc -c | pretty-bytes)",
    "generate-docs": "jsdoc -c ./conf.json -d generated-docs -t ./node_modules/ink-docstrap/template -R docs/README.md -r ./src/**",
    "pretest": "yarn lint && yarn build",
    "test": "jest --coverage --coveragePathIgnorePatterns *.min.js",
    "coveralls": "jest --config jest_coveralls.json --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "start": "webpack-dev-server --progress --colors"
  },
  "bugs": {
    "url": "http://github.com/seanoshea/fuzzymatchingjs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seanoshea/fuzzymatchingjs.git"
  },
  "keywords": [
    "fuzzy",
    "match",
    "string",
    "matching"
  ],
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-runtime": "^6.23.0",
    "coveralls": "^2.11.9",
    "eslint": "^3.8.1",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.0.1",
    "ink-docstrap": "^1.3.0",
    "jest": "^20.0.4",
    "jsdoc": "^3.4.3",
    "prettier": "^1.3.1",
    "pretty-bytes": "^4.0.2",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.5.1",
    "uglify-js": "^3.0.11",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "author": "Sean O'Shea <oshea.ie@gmail.com>",
  "license": "Apache-2.0",
  "homepage": "https://github.com/seanoshea/fuzzymatchingjs#readme"
}
